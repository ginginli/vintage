{
    "version": 2,
    "builds": [
        { "src": "api/**/*.js", "use": "@vercel/node" },
        { "src": "public/**", "use": "@vercel/static" }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "has": [
                {
                    "type": "host",
                    "value": "vintage-laineys-projects.vercel.app"
                }
            ],
            "status": 301,
            "headers": {
                "Location": "https://stockanalyse.site/$1"
            }
        },
        {
            "src": "/(.*)",
            "has": [
                {
                    "type": "host",
                    "value": "vintage-git-main-laineys-projects.vercel.app"
                }
            ],
            "status": 301,
            "headers": {
                "Location": "https://stockanalyse.site/$1"
            }
        },
        {
            "src": "/sitemap.xml",
            "dest": "/public/sitemap.xml"
        },
        {
            "src": "/robots.txt",
            "dest": "/public/robots.txt"
        },
        {
            "src": "/api/(.*)",
            "dest": "/api/$1.js"
        },
        {
            "handle": "rewrite",
            "source": "^/www\\.(.*)",
            "destination": "/$1",
            "status": 301,
            "headers": {
                "Location": "https://stockanalyse.site/$1"
            }
        },
        {
            "src": "/(.*)",
            "dest": "/public/$1"
        }
    ],
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "X-Robots-Tag",
                    "value": "index, follow"
                },
                {
                    "key": "Link",
                    "value": "<https://stockanalyse.site>; rel=\"canonical\""
                }
            ]
        }
    ]
}
